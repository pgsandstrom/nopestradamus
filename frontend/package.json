{
  "name": "nopestradamus-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "format": "prettier --write \"{,!(.next|node_modules)}/**/*.{json,md,ts,tsx,css,scss}\"",
    "lint": "node ./node_modules/eslint/bin/eslint.js --ext .ts --ext .tsx --fix .",
    "pre-commit": "lint-staged",
    "pre-push": "npm run lint && npm run typecheck",
    "start": "next start",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@types/node": "14.0.27",
    "@types/react": "16.9.46",
    "cross-env": "7.0.2",
    "next": "9.5.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "typescript": "4.0.1-rc"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "3.9.0",
    "@typescript-eslint/parser": "3.9.0",
    "eslint": "7.6.0",
    "eslint-config-prettier": "6.11.0",
    "lint-staged": "10.2.11",
    "prettier": "2.0.5"
  },
  "lint-staged": {
    "*.{json,md,css,scss}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "node ./node_modules/eslint/bin/eslint.js --ext .ts --ext .tsx --fix",
      "prettier --write"
    ]
  }
}
